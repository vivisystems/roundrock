msgstr "Cancel"

##################
# Product Search
##################

msgid "plusearch.desc"
msgstr ""
"This function allows the operator to perform a Product search. No parameter "
"is needed."

msgid "plusearch.label"
msgstr "Product Search"

msgid "plusearch.name"
msgstr "Product Search"

###############
# Queue Order
###############

msgid "pushqueue.desc"
msgstr ""
"This function queues the current order and readies the register for a new "
"order. No parameter is needed."

msgid "pushqueue.label"
msgstr "Queue Order"

msgid "pushqueue.name"
msgstr "Queue Order"

##############
# View Order
##############

msgid "pullqueue.desc"
msgstr ""
"This function allows the operator to recall a queued order. No parameter is "
"needed."

msgid "pullqueue.label"
msgstr "View Queue"

msgid "pullqueue.name"
msgstr "View Queue"

###############
# Modify Item
###############

msgid "modifyitem.desc"
msgstr ""
"This function allows the operator to modify a registered item. No parameter "
"is needed."

msgid "modifyitem.label"
msgstr "Modify"

msgid "modifyitem.name"
msgstr "Modify Item"

#############
# Void Item
#############

msgid "voidItem.desc"
msgstr "This function voids the last transaction. No parameter is needed."

msgid "voidItem.label"
msgstr "VOID"

msgid "voidItem.name"
msgstr "VOID"

#############
# House BON
#############

msgid "houseBon.desc"
msgstr ""
"This function marks the current item as 'on-the-house'. The parameter, if "
"specified, will be used to register the discount."

msgid "houseBon.label"
msgstr "House BON"

msgid "houseBon.name"
msgstr "House BON"

############
# Sign Off
############

msgid "signOff.desc"
msgstr "This function signs off the current operator. No parameter is needed."

msgid "signOff.label"
msgstr "Sign Off"

