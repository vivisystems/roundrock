                </row>
                <row>
                    <label value="&training.taketrainingdbtobedefaultdb;"/>
                    <button id="take_training_db_to_be_default_db" disabled="false" class="button-execute button-enable" oncommand="$do( 'takeTrainingDBToBeDefaultDB', null, 'TrainingMode' );"/>
                </row>
            </rows>
        </grid>

    </prefpane>

