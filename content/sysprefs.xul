                </row>
                <row>
                    <checkbox id="requirecashdeclaration" label="&shift.requirecashdeclaration;" preference="pref_requirecashdeclaration"/>
                    <checkbox id="checksaleperiod" label="&shift.checksaleperiod;" preference="pref_checksaleperiod"/>
                </row>
                <row>
                    <checkbox id="hideshiftdialog" label="&shift.hideshiftdialog;" preference="pref_hideshiftdialog"/>
                </row>
