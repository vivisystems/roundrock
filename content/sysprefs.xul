    <prefpane id="orderDataSettingsPane" label="&category.orderdata;" >
        <preferences>

            <preference id="pref_orderretaindays" name="vivipos.fec.settings.OrderRetainDays" type="int"/>
            <preference id="pref_orderweeklypack" name="vivipos.fec.settings.OrderWeeklyPack" type="int"/>

