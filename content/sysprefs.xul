    <prefpane id="generalSettingsPane" label="&category.general;" >
        <preferences>
            <preference id="pref_publicattendance" name="vivipos.fec.settings.PublicAttendance" type="bool" />
            <preference id="pref_defaultpricelevel" name="vivipos.fec.settings.DefaultPriceLevel" type="int" />

            <preference id="pref_activepricelevelschedule" name="vivipos.fec.settings.ActivePriceLevelSchedule" type="bool" />
            <preference id="pref_pricelevelrevert" name="vivipos.fec.settings.PriceLevelRevert" type="bool" />
            <preference id="pref_changedepartmentclearbuffer" name="vivipos.fec.settings.ChangeDepartmentClearBuffer" type="bool" />
            <preference id="pref_allowoverstock" name="vivipos.fec.settings.AllowOverStock" type="bool" />
            <preference id="pref_viewlocalordersonly" name="vivipos.fec.settings.ViewLocalOrdersOnly" type="bool" />
            <preference id="pref_ageverificationack" name="vivipos.fec.settings.AgeVerificationAck" type="bool" />
            <preference id="pref_positivepricerequired" name="vivipos.fec.settings.PositivePriceRequired" type="bool" />
            <preference id="pref_clearcartafterfinalization" name="vivipos.fec.settings.ClearCartAfterFinalization" type="bool" />
            <preference id="pref_maxsaleperiodleaddays" name="vivipos.fec.settings.MaxSalePeriodLeadDays" type="int" />
            <preference id="pref_idletime" name="vivipos.fec.settings.idle.time" type="int" />
            <preference id="pref_collapsecondiments" name="vivipos.fec.settings.collapse.condiments" type="bool" />
            <preference id="pref_departmentpopstotop" name="vivipos.fec.settings.department.pops.to.top" type="bool" />
            <preference id="pref_quietcancel" name="vivipos.fec.settings.quietcancel" type="bool" />
            <preference id="pref_clockinenabled" name="vivipos.fec.settings.ClockInEnabled" type="bool" />


        </preferences>

        <grid class="DataForm">
            <columns>
                <column flex="1" /><column flex="1" />
            </columns>
            <rows>
                <row>
                    <checkbox preference="pref_activepricelevelschedule" id="pref_activepricelevelscheduleCtrl" label="&general.activepricelevelschedule;" />
                    <checkbox preference="pref_pricelevelrevert" id="pref_pricelevelrebackCtrl" label="&general.pricelevelrevert;" />
                </row>
                <row>
                    <checkbox preference="pref_changedepartmentclearbuffer" id="pref_changedepartmentclearbufferCtrl" label="&general.changedepartmentclearbuffer;" />
                    <checkbox preference="pref_clearcartafterfinalization" id="pref_clearcartafterfinalizationCtrl" label="&general.clearcartafterfinalization;" />
                </row>
                <row>
                    <checkbox preference="pref_ageverificationack" id="pref_ageverificationackCtrl" label="&general.ageverificationack;" />
                    <checkbox preference="pref_positivepricerequired" id="pref_positivepricerequiredCtrl" label="&general.positivepricerequired;" />
                </row>
                <row>
                    <checkbox preference="pref_allowoverstock" id="pref_allowoverstockCtrl" label="&general.allowoverstock;" />
                    <checkbox preference="pref_viewlocalordersonly" id="pref_viewlocalordersonlyCtrl" label="&general.viewlocalordersonly;" />
                </row>
                <row>
                    <checkbox preference="pref_departmentpopstotop" id="pref_departmentpopstotopCtrl" label="&general.departmentpopstotop;" />
                    <checkbox preference="pref_collapsecondiments" id="pref_collapsecondimentsCtrl" label="&general.collapsecondiments;" />
                </row>
                <row>
                    <checkbox preference="pref_publicattendance" id="pref_publicAttendanceCtrl" label="&general.publicattendance;" />
                    <checkbox preference="pref_clockinenabled" id="pref_clockinenabledCtrl" label="&general.showclockinicon;" />
                </row>
                <row>
                    <hbox>
                        <label control="idletime" value="&general.idletime;"/>
                        <textbox preference="pref_idletime" id="idletime" type="number" size="3" maxlength="3" min="60" max="999"/>
                    </hbox>
                    <checkbox preference="pref_quietcancel" id="quietcancel" label="&general.quietcancel;" />
                </row>
                <row>
                    <vbox>
                        <label control="defaultpricelevel" value="&general.defaultpricelevel;"/>
                        <textbox preference="pref_defaultpricelevel" id="defaultpricelevel" type="number" size="2" maxlength="1" min="1" max="9"/>
                    </vbox>
                    <vbox>
                        <label control="maxsaleperiodleaddays" value="&general.maxsaleperiodleaddays;"/>
                        <textbox preference="pref_maxsaleperiodleaddays" id="maxsaleperiodleaddays" type="number" size="3" min="1"/>
                    </vbox>
                </row>
            </rows>
        </grid>
    </prefpane>
    

    <prefpane id="formatSettingsPane" label="&category.formats;" >
        <preferences>

            <preference id="pref_prodnoautogenerate" name="vivipos.fec.settings.AutoGenerateProdNo" type="bool"/>
            <preference id="pref_prodnolength" name="vivipos.fec.settings.ProdNoLength" type="int"/>
            <preference id="pref_decimalpoint" name="vivipos.fec.settings.DecimalPoint" type="unichar"/>
            <preference id="pref_thousandsdelimiter" name="vivipos.fec.settings.ThousandsDelimiter" type="unichar"/>
            <preference id="pref_precisionprices" name="vivipos.fec.settings.PrecisionPrices" type="int"/>
            <preference id="pref_precisiontaxes" name="vivipos.fec.settings.PrecisionTaxes" type="int"/>
            <preference id="pref_sequencetrackssaleperiod" name="vivipos.fec.settings.SequenceTracksSalePeriod" type="bool" />
            <preference id="pref_sequencenumberlength" name="vivipos.fec.settings.SequenceNumberLength" type="int" />
            <preference id="pref_revalueprices" name="vivipos.fec.settings.RevaluePrices" type="int"/>
            <preference id="pref_autorevalueprices" name="vivipos.fec.settings.AutoRevaluePrices" type="bool"/>


        </preferences>
        <vbox>
            <grid class="DataForm">
                <columns>
                    <column flex="1" />
                    <column flex="1" />
                </columns>
                <rows>
                    <row>
                        <checkbox id="prodnoautogenerate" label="&formats.autogenerate;" preference="pref_prodnoautogenerate"/>
                        <vbox>
                            <label control="prodnolength" value="&formats.prodnolength;"/>
                            <textbox preference="pref_prodnolength" id="prodnolength" type="number" min="2"/>
                        </vbox>
                    </row>
                    <row>
                        <checkbox preference="pref_sequencetrackssaleperiod" id="pref_sequencetrackssaleperiodCtrl" label="&formats.sequencetrackssaleperiod;" />
                        <vbox>
                            <label control="sequencenumberlength" value="&formats.sequence.number.length;"/>
                            <textbox preference="pref_sequencenumberlength" id="sequencenumberlength" type="number" min="2" max="6"/>
                        </vbox>
                    </row>

                    <row>
                        <vbox>
                            <label control="roundingtaxes" value="&formats.roundingtaxes;"/>
                            <menulist id="roundingtaxes" flex="1" oncommand="GeckoJS.Configure.write('vivipos.fec.settings.RoundingTaxes', this.value)">
                                <menupopup>
                                    <menuitem label="&formats.rounding.to-nearest-precision;" value="to-nearest-precision"/>
                                    <!--
                                    <menuitem label="&formats.rounding.to-nearest-half;" value="to-nearest-half"/>
                                    <menuitem label="&formats.rounding.to-nearest-quarter;" value="to-nearest-quarter"/>
                                    <menuitem label="&formats.rounding.to-nearest-nickel;" value="to-nearest-nickel"/>
                                    <menuitem label="&formats.rounding.to-nearest-dime;" value="to-nearest-dime"/>
                                    -->
                                    <menuitem label="&formats.rounding.always-round-up;" value="always-round-up"/>
                                    <menuitem label="&formats.rounding.always-round-down;" value="always-round-down"/>
                                </menupopup>
                            </menulist>
                        </vbox>
                        <vbox>
                            <label control="precisiontaxes" value="&formats.precisiontaxes;"/>
                            <textbox preference="pref_precisiontaxes" id="precisiontaxes" type="number" min="-9" max="9"/>
                        </vbox>
                    </row>
                    <row>
                        <vbox>
                            <label control="roundingprices" value="&formats.roundingprices;"/>
                            <menulist id="roundingprices" flex="1" oncommand="GeckoJS.Configure.write('vivipos.fec.settings.RoundingPrices', this.value)">
                                <menupopup>
                                    <menuitem label="&formats.rounding.to-nearest-precision;" value="to-nearest-precision"/>
                                    <!--
                                    <menuitem label="&formats.rounding.to-nearest-half;" value="to-nearest-half"/>
                                    <menuitem label="&formats.rounding.to-nearest-quarter;" value="to-nearest-quarter"/>
                                    <menuitem label="&formats.rounding.to-nearest-nickel;" value="to-nearest-nickel"/>
                                    <menuitem label="&formats.rounding.to-nearest-dime;" value="to-nearest-dime"/>
                                    -->
                                    <menuitem label="&formats.rounding.always-round-up;" value="always-round-up"/>
                                    <menuitem label="&formats.rounding.always-round-down;" value="always-round-down"/>
                                </menupopup>
                            </menulist>
                        </vbox>
                        <vbox>
                            <label control="precisionprices" value="&formats.precisionprices;"/>
                            <textbox preference="pref_precisionprices" id="precisionprices" type="number" min="-9" max="9"/>
                        </vbox>
                    </row>
                    <row>
                        <vbox>
                            <label control="decimalpoint" value="&formats.decimalpoint;"/>
                            <textbox preference="pref_decimalpoint" id="decimalpoint" size="1" maxlength="1" default="."/>
                        </vbox>
                        <vbox>
                            <label control="thousandsdelimiter" value="&formats.thousandsdelimiter;"/>
                            <textbox preference="pref_thousandsdelimiter" id="thousandsdelimiter" size="1" maxlength="1" default=","/>
                        </vbox>
                    </row>
                    <row>
                        <checkbox id="autorevalueprices" label="&formats.autorevalueprices;" preference="pref_autorevalueprices"/>
                        <vbox>
                            <label control="revalueprices" value="&formats.revalueprices;"/>
                            <textbox preference="pref_revalueprices" id="revalueprices" type="number" min="0"/>
                        </vbox>
                    </row>
                </rows>
            </grid>
        </vbox>
    </prefpane>

    <prefpane id="loginSettingsPane" label="&category.login;" >
        <preferences>
            <preference id="pref_autodiscardcart" name="vivipos.fec.settings.autodiscardcart" type="bool"/>
            <preference id="pref_autodiscardqueue" name="vivipos.fec.settings.autodiscardqueue" type="bool"/>
