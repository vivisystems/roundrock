            </columns>
            <rows>
                <row>
                    <checkbox id="allowquicklogin" label="&login.allowquicklogin;" preference="pref_allowquicklogin"/>
                </row>
                <row>
                    <vbox>
                        <checkbox preference="pref_defaultlogin" id="pref_defaultloginCtrl" label="&login.defaultlogin;" />
                        <viviscrollablebuttonpanel dir="normal" id="userscrollablepanel" flex="1" cols="5" rows="2" labelField="displayname"
                                               oncommand="$do('select',this.selectedIndex, 'Users');"
                                               class="ItemScrollablepanel WidthSet"
                                               buttonClass="ItemButtonA"
                                               scrollUpClass="ScrollUp"
                                               scrollDownClass="ScrollDown">
                            <spacer flex="1"/>
                        </viviscrollablebuttonpanel>
                    </vbox>
                </row>
            </rows>
            </grid>

            <grid class="DataFormB">
                <columns>
                    <column flex="1" /><column flex="1" />
                </columns>
                <rows>
                    <row>
                        <checkbox id="autodiscardcart" label="&login.autodiscardcart;" preference="pref_autodiscardcart"/>
                        <checkbox id="autodiscardqueue" label="&login.autodiscardqueue;" preference="pref_autodiscardqueue"/>
                    </row>
                    <row>
                        <checkbox id="mustemptyqueue" label="&login.mustemptyqueue;" preference="pref_mustemptyqueue"/>
                    </row>
                </rows>
            </grid>
        </vbox>
    </prefpane>

    <prefpane id="taxSettingsPane" label="&category.tax;" >
        <preferences>
        </preferences>
        <vbox class="TopArea">
