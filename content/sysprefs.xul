    <prefpane id="orderDataSettingsPane" label="&category.orderdata;">
        <preferences>
            <preference id="pref_orderretaindays" name="vivipos.fec.settings.OrderRetainDays" type="int"/>
            <preference id="pref_orderweeklypack" name="vivipos.fec.settings.OrderWeeklyPack" type="int"/>
            <preference id="pref_autobackuponclosing" name="vivipos.fec.settings.AutoBackupOnClosing" type="bool"/>
            <preference id="pref_inventory_adjustment_retaindays" name="vivipos.fec.settings.InventoryAdjustmentRetainDays" type="int"/>
            <preference id="pref_orderexpirehistory" name="vivipos.fec.settings.moveExpiredDataToHistory" type="bool"/>
            <preference id="pref_splitHistoryOrderFile" name="vivipos.fec.settings.splitHistoryOrderFile" type="unichar"/>
        </preferences>

        <grid class="data-form data-form-options">
            <columns>
                <column flex="1"/>
                <column flex="1"/>
            </columns>
            <rows>
                <row>
                    <label value="&orderdata.diskunused;"/>
                    <label id="disk_unused" value="100MB"/>
                </row>
                <row>
                    <label value="&orderdata.orderretaindays;"/>
                    <textbox preference="pref_orderretaindays" id="orderRetaindays" type="number" size="4" maxlength="4" min="0" max="9999"/>
                </row>
                <row>
                    <label value="&orderdata.orderweeklypack;"/>
                    <menulist preference="pref_orderweeklypack" id="orderweeklypack" flex="1">
                        <menupopup id="weekmenulist">
                        </menupopup>
                    </menulist>
                </row>
                <row>
                    <checkbox preference="pref_orderexpirehistory" id="orderexpirehistory" label="&orderdata.move_order_to_history;"/>
                    <hbox>
                        <label value="&orderdata.split_history_order_file;" />
                         <menulist flex="1" preference="pref_splitHistoryOrderFile" id="splitHistoryOrderFile">
                            <menupopup>
                              <menuitem label="&orderdata.split_history_order_file_monthly;" value="monthly"/>
                              <menuitem label="&orderdata.split_history_order_file_quarterly;" value="quarterly"/>
                              <menuitem label="&orderdata.split_history_order_file_halfyearly;" value="halfyearly"/>
                              <menuitem label="&orderdata.split_history_order_file_yearly;" value="yearly"/>
                            </menupopup>
                          </menulist>
                    </hbox>
                </row>
                <row>
                    <label value="&orderdata.inventory.adjustment.retaindays;"/>
                    <textbox preference="pref_inventory_adjustment_retaindays" id="inventoryAdjustmentRetaindays" type="number" size="4" maxlength="4" min="0" max="9999"/>
                </row>
                <row>
                    <checkbox preference="pref_autobackuponclosing" id="autobackuponclosing" label="&orderdata.autobackuponclosing;" />
                </row>
            </rows>
        </grid>

        <vbox align="center" pack="start" flex="1">
            <diskspace_usage flex="1"/>
        </vbox>
        
        <spacer height="5px"/>

        <hbox pack="center" class="toolbar" toolbarmode="horizontal">
            <button id="rebuild_database" disabled="false" label="&orderdata.rebuilddatabase;" oncommand="rebuildDatabases()"/>
        </hbox>
        
    </prefpane>
    
    <prefpane id="trainingSettingsPane" label="&category.training;">
        <preferences>
            <preference id="pref_ifprintwhiletraining" name="vivipos.fec.settings.ifprintwhiletraining" type="bool"/>
