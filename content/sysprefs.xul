    <prefpane id="shiftSettingsPane" label="&category.shift;">
        <preferences>
            <preference id="pref_disablesaleperiod" name="vivipos.fec.settings.DisableSalePeriod" type="bool"/>
            <preference id="pref_disableshiftchange" name="vivipos.fec.settings.DisableShiftChange" type="bool"/>
            <preference id="pref_requirecashdeclaration" name="vivipos.fec.settings.RequireCashDeclaration" type="bool"/>
            <preference id="pref_defaultchangeindrawer" name="vivipos.fec.settings.DefaultChangeInDrawer" type="int"/>
