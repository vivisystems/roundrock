      <label id="inputtext" value="Please Select the Queue Order..." style="font-size:28px;"/>
      <label id="user_name" value=""/>
      <hbox>
        <spacer flex="1" />
        <vbox>
          <label id="" value="Queue List:" />
          <!-- simplelistbox id="queueSimpleListBox" flex="1" rows="24"  headers="User No,User Name" style="width:300px; height:450px;"/ -->
