        </vbox>
        <spacer flex="1" />
        <vbox flex="1">
          <label id="" value="Item List..." />
          <textbox readonly="1" id="itemlist" value="" style="height: 300px; width: 320px;"/>
        </vbox>
        <spacer flex="1" />
      </hbox>
      <spacer flex="1" />

      <hbox id="okCancelButtonsRight" class="dialogbtns" />

    </vbox>
  </box>
