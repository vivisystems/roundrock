<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>${package_id}@firich.com.tw</em:id>
        <em:name>${package_name}</em:name>
        <em:version>${package_version}</em:version>
        <em:description>${package_description}</em:description>
        <em:creator>${package_creator}</em:creator>
        <em:homepageURL>http://www.firich.com.tw/</em:homepageURL>
        <em:type>2</em:type> <!-- type=extension --> 

        <em:targetApplication>
          <!-- Toolkit -->
          <Description>
            <em:id>toolkit@mozilla.org</em:id>
            <em:minVersion>1.9a1</em:minVersion>
            <em:maxVersion>1.9.*</em:maxVersion>
          </Description>
        </em:targetApplication>
        
    </Description>
</RDF>

