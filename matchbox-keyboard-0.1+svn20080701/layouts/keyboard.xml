    <space width="800" />

    <key width="1000"  extended="true">
         <default display="image:/usr/share/pixmaps/close.png" 
	          action="close"/>
    </key>
    <space fill="true" />

  </row>
  <row>
    <space width="5" />
    <key width="3500"  extended="true">
