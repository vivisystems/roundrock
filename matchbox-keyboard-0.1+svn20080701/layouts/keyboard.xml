    <space width="50" />

    <key width="2800"  extended="true">
	  <default display="PgDn" action="pagedown"/>
    </key>
    <key width="2800"  extended="true">
	  <default display="End" action="end"/>
    </key>

    <space width="50" />

    <space fill="true" />

  </row>
  <row>
    <space width="5" />
    <key width="3500">
           <default display="Shift" colortext="#95BFFC" action="modifier:shift"/>
           <shifted display="SHIFT" colortext="#FFFFFF" action="modifier:shift"/>
    </key >
    <space width="50" />

    <key obey-caps="true">
	  <default display="z" />                
	  <shifted display="Z" />
    </key>

    <key obey-caps="true">
	  <default display="x" />                
	  <shifted display="X" />
    </key>

    <key obey-caps="true">
	  <default display="c" />                
	  <shifted display="C" />
    </key>

    <key obey-caps="true">
	  <default display="v" />                
	  <shifted display="V" />
    </key>

    <key obey-caps="true">
