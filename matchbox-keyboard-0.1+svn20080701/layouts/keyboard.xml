    <space width="50"/>
    
    <key obey-caps="true">
	  <default display="q" />                
	  <shifted display="Q" />
    </key>
    <key obey-caps="true">
	  <default display="w" />                
	  <shifted display="W" />
    </key>
    <key obey-caps="true">
