          <mod2 display="±" />
    </key>
    <key>
	  <default display="8" colorbackground="#000000" />
	  <shifted display="*" colorbackground="#000000" />
          <mod1    display="" />
          <mod2 display="µ" />
    </key>
    <key>
	  <default display="9" colorbackground="#000000" />
	  <shifted display="(" colorbackground="#000000" />
          <mod1    display="" />
          <mod2 display="¶" />
    </key>
    <space width="2550" />

    <key width="1000"  extended="true">
	  <default display="image:/usr/share/pixmaps/arrow_up.png"  
                   colorbackground="#000000" action="up"/>
    </key>

    <space width="2550" />
    <key width="3000"  extended="true">
          <default display="äëö" colorbackground="#000000" 
                   colortext="#95BFFC" action="modifier:mod1"/>
    </key>


    <space fill="true" />
 </row>
  <row>
    <space width="5" />

    <key width="3500"  >
	  <default display="Caps" colorbackground="#000000"
                   colortext="#95BFFC" action="modifier:caps"/>
    </key>

    <space width="50"/>

    <key  extended="true">
	  <default display="Ctrl" colorbackground="#000000" 
                   colortext="#95BFFC" action="modifier:ctrl"/>
    </key>
    <key  >
	  <default display="Alt" colorbackground="#000000" 
                   colortext="#95BFFC"  action="modifier:alt"/> 
    </key>

    <key width="10000" extended="true" >
	  <default display=" " action="space" />    
    </key>

    <key  extended="true">
	  <default display="`" />
	  <shifted display="~" />
    </key>

    <key>
	  <default display="[" />    
	  <shifted display="{" />
    </key>
    <key>
	  <default display="]" />    
	  <shifted display="}" />
    </key>
    
    <space width= "800" />

    <key>
	  <default display="0" colorbackground="#000000" />
	  <shifted display=")" colorbackground="#000000" />    
	  <mod2    display="»" />
    </key>
    <key>
	  <default display="-" colorbackground="#000000" />
